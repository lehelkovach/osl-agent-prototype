OPENAI_API_KEY=your-openai-key
OPENAI_CHAT_MODEL=gpt-4o
OPENAI_EMBEDDING_MODEL=text-embedding-3-large

# GitHub (optional; prefer `gh auth login` locally or Cursor Secrets in cloud)
GITHUB_TOKEN=your-github-token

# ArangoDB connection (optional)
ARANGO_URL=https://your-arango-host:8529
ARANGO_DB=agent_memory
ARANGO_USER=root
ARANGO_PASSWORD=your-password
ARANGO_VERIFY=/path/to/ca-bundle.crt  # set to path, or "false" to disable verify (not recommended)
